package chapter06;

public class TeamGroup {

	private TeamMember_05[] member = new TeamMember_05[5];

	public void init() {

		member[0] = new TeamMember_05("신인철", "남학생");
		member[1] = new TeamMember_05("김관호", "남학생");
		member[2] = new TeamMember_05("유연우", "여학생");
		member[3] = new TeamMember_05("임해균", "남학생");
		member[4] = new TeamMember_05("문원주", "남학생");

	} // init

	public void Disp() {
		for (int i = 0; i < member.length; i++) {
			System.out.print("성명: " + (member[i].getName()) + " ");
			System.out.println("성별: " + (member[i].getGender()));
		}
	} // Disp

} // class
