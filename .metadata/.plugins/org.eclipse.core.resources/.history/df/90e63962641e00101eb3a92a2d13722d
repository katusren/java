package chapter06;

public class Example_person_Main {

	public static void main(String[] args) {

		// 이름:홍길동 키:180 몸무게:80
		
		Example_person per = new Example_person();
		
		per.name = "홍길동";
		per.height = 180;
		per.weight = 80;
		
		System.out.println("이름: "+per.name);
		System.out.println("키: "+per.height);
		System.out.println("몸무게: "+per.weight);
	}

}
